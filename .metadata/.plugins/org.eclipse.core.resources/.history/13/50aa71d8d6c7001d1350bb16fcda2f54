	}
	public ArrayList<Subject> getStudentScore() {
		return studentScore;
	}
	public void setStudentScore(ArrayList<Subject> studentScore) {
		this.studentScore = studentScore;
	}
	
	public void showStudentInfo() {
		
	}
	
	public void addSubject() {
		
	}
}
