package ch12;

public class Factory {
	
	private static int carNum = 10000;
	
	private static Factory instance = new Factory();
	
	public void createCar(Car car) {
		carNum++;
	}
	
	public static Factory getInstance() {
		if(instance == null) {
			instance = new Factory();
		}
		return instance;
	}
	
}
