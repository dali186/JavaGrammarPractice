package ch12;

public class Car {

	private int carNum;
	private String carName;
	
	public int getCarNum() {
		return carNum;
	}
	public void setCarNum(int carNum) {
		this.carNum = carNum;
	}
	public String getCarName() {
		return carName;
	}
	public void setCarName(String carName) {
		this.carName = carName;
	}
	
	public Car(int carNum, String carName) {
		this.carNum = carNum;
		this.carName = carName;
	}
	
}
